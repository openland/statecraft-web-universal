  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/react": "^16.0.27",
    "@types/react-dom": "^16.0.3",
    "next": "^4.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "scripts": {
    "prebuild": "tsc",
    "build": "yarn next build ./build/dist",
    "predev": "tsc",
    "dev": "concurrently \"yarn build:ts:watch\" \"yarn next ./build/dist\"",
    "start": "yarn next start ./build/dist",
    "build:ts": "tsc",
    "build:ts:watch": "tsc --watch"
  }
