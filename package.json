{
  "name": "statecraft-web-universal",
  "version": "1.0.0",
  "repository": "https://github.com/statecrafthq/statecraft-web-universal.git",
  "author": "Steve Kite <korshakov.stepan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=v8.0.0 <9.0.0",
    "yarn": ">=1.0.0"
  },
  "dependencies": {
    "@types/auth0-js": "^8.11.0",
    "@types/js-cookie": "^2.1.0",
    "@types/markdown-it": "^0.0.4",
    "@types/marked": "^0.3.0",
    "@types/next": "^2.4.6",
    "@types/nprogress": "^0.0.29",
    "@types/prop-types": "^15.5.2",
    "@types/query-string": "^5.0.1",
    "@types/react": "^16.0.27",
    "@types/react-dom": "^16.0.3",
    "@types/react-router": "^4.0.19",
    "@types/react-router-dom": "^4.2.3",
    "auth0-js": "^8.12.0",
    "exenv": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "markdown-it": "^8.4.0",
    "marked": "^0.3.7",
    "next": "^4.2.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-apollo": "1.4.16",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "typescript": "^2.6.2",
    "webpack-bundle-analyzer": "^2.9.1",
    "cpx": "^1.5.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0"
  },
  "scripts": {
    "prebuild": "./node_modules/typescript/bin/tsc & yarn assets",
    "build": "yarn next build ./build/dist",
    "predev": "./node_modules/typescript/bin/tsc",
    "assets": "yarn cpx \"./src/static/**/*\" ./build/dist/static/",
    "assets:watch": "yarn cpx \"./src/static/**/*\" ./build/dist/static/ --watch",
    "dev": "concurrently \"yarn build:ts:watch\" \"yarn next ./build/dist\" \"yarn assets:watch\"",
    "start:prod": "yarn next start ./build/dist",
    "start": "yarn next start ./build/dist -p $PORT",
    "build:ts": "./node_modules/typescript/bin/tsc",
    "build:ts:watch": "./node_modules/typescript/bin/tsc --watch",
    "export": "yarn next export ./build/dist",
    "analyze": "cross-env ANALYZE=1 next build ./build/dist",
    "heroku-postbuild": "yarn build"
  }
}
